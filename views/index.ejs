<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1.0" />
		<title>Message Board</title>
		<link rel="stylesheet" href="./styles.css" />
	</head>
	<header>
		<h1>Public Message Board</h1>
	</header>
	<body>
		<div class="wrapper">
			<ul>
				<% messages.forEach((msg) => { %> <%- include('../templates/message', {id:
				msg.id, title: msg.title, user: msg.user, text: msg.text, added:
				msg.added}); %> <% }); %>
			</ul>
		</div>
	</body>
	<footer>
		<a target="_blank" href="https://github.com/Spaceflyy"
			>Spaceflyy <img src="./github-mark.svg" alt=""
		/></a>
	</footer>
</html>
